{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "public": true,
  "routes": [
    {
      "src": "/data/(.*)",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "s-maxage=3600"
      },
      "dest": "/data/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
