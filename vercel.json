{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/static/$1"
    },
    { 
      "src": "/[^.]+", 
      "dest": "/", 
      "status": 200 
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}